	"main": "src/index.js",
	"scripts": {
		"test:unit": "jest --coverage",
		"coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
		"dev": "ts-node-dev --respawn --pretty --transpile-only src/bin/www.ts",
		"start": "npx ts-node src/bin/www.ts",
		"mon": "npx nodemon src/bin/www.ts",
